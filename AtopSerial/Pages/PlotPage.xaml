<Page x:Class="AtopSerial.Pages.PlotPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
    xmlns:local="clr-namespace:AtopSerial.Pages"
    xmlns:wpf="clr-namespace:ScottPlot.WPF;assembly=ScottPlot.WPF"
    mc:Ignorable="d" 
    d:DesignWidth="800"
    d:DesignHeight="650" 
    Title="PlotPage"
    Loaded="Page_Loaded">
    <Grid AllowDrop="True" Drop="Plot_Drop">
        <WpfPlot Name="Plot" MouseMove="Plot_MouseMove"/>
    </Grid>
</Page>
